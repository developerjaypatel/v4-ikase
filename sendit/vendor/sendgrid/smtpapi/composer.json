{
  "name": "sendgrid/smtpapi",
  "description": "Build SendGrid X-SMTPAPI headers in PHP.",
  "version": "0.0.1",
  "homepage": "http://github.com/sendgrid/smtpapi-php",
  "license": "MIT",
  "keywords": ["SendGrid", "sendgrid", "email", "send", "grid", "smtpapi", "smtp", "api", "xsmtp", "X-SMTP"],
  "require": {
    "php": ">=5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*"
  },
  "replace": {
    "sendgrid/smtpapi-php": "*"
  },
  "type": "library",
  "autoload": {
    "psr-0": {"Smtpapi": "lib/"}
  }
},
{
  "name": "fcosrno/sendgrid-report",
  "description": "PHP wrapper to view and manage SendGrid reports through the SendGrid Web API.",
  "homepage": "http://github.com/fcosrno/sendgrid-report-php",
  "license": "MIT",
  "keywords": ["SendGrid","sendgrid","Reports", "Spam","Bounces","Blocks","Invalid Emails","Unsubscribes"],
  "require": {
    "php": ">=5.3",
    "sendgrid/sendgrid":"2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.*",
    "phpdocumentor/phpdocumentor":"2.*"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Fcosrno\\SendGridReport\\": "src/"
    }
  }
}

